#include<bits/stdc++.h>
using namespace std;
int main()
{
  int n;
  cin>>n;
  int cnt=0;
  for(int i=0;i<n;i++)
  {
      int x=0;
      int a[3];
      for(int j=0;j<3;j++)
      {cin>>a[j];
      if(a[j]==1)
        x++;
      }
      if(x>=2)
        cnt++;



  }
  cout<<cnt<<endl;
  return 0;
}
